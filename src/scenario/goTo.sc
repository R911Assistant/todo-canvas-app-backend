theme: /

    state: ПереходНа
        q!: (~перейти/~открыть/~переход/~показать/показ*/покажи/отобраз*) * [~отчет|~график]
            $AnyText::anyText
            
        script:
            goTo($parseTree._anyText, $context);
        a:  Ок1
        
        
    state: ПереходДальше
        q!: [~перейти/~открыть/~переход/~показать/показ*/покажи/отобраз*] [к] (дальше/далее/~следующий/вперёд) * [~отчет|~график]

        script:
            goToNext($context);
        a: Ладно
            
        go!: /ПереходимНаСледующуюСтраницу
        
        
    state: ПереходНазад
        q!: {[~перейти/~открыть/~переход/~показать/показ*/покажи/отобраз*] [к] (назад/~предыдущий/вернись) * [~отчет|~график]}

        script:
            goToBack($context);
            
        a: преды
        
        go!: /ПереходимНаПредыдущуюСтраницу
        
    state: ПереходКРазделу
        q!: (~перейти/~открыть/~переход/~показать/показ*/покажи/отобраз*) [к] (раздел*/секци*) 
            $AnyText::anyText
            
        script:
            goToAnchor($parseTree._anyText, $context);
        
        a: ffff
        